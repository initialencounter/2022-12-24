{
  "name": "@koishijs/boilerplate",
  "version": "0.0.0",
  "packageManager": "yarn@4.4.0",
  "private": true,
  "files": [
    ".env",
    "koishi.yml"
  ],
  "license": "AGPL-3.0",
  "workspaces": [
    "plugins/Adapter/*",
    "plugins/AI/*",
    "plugins/Behavior/*",
    "plugins/Console/*",
    "plugins/Extension/*",
    "plugins/Games/*",
    "plugins/Manager/*",
    "plugins/News/*",
    "plugins/Recreation/*",
    "plugins/Tool/*",
    "plugins/@*/*",
    "external/open-vits",
    "external/test",
    "external/transformers/transformers",
    "external/transformers/anti-nsfw",
    "external/resvg"
  ],
  "scripts": {
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "clone": "koishi-scripts clone",
    "build": "yakumo build",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register",
    "start": "koishi start",
    "set-proxy": "npm config set registry https://registry.npmmirror.com",
    "unset-proxy": "npm config delete registry",
    "clean": "node -r esbuild-register scripts/clean.ts",
    "build:help": "powershell Remove-item ./plugins/Manager/help-pro/lib -Recurse -Force && powershell Copy-Item -Path ./plugins/Manager/help-pro/src/static -Destination ./plugins/Manager/help-pro/lib/static -Recurse -Force && yarn build help-pro",
    "test:gh-tile": "mocha --exit -r esbuild-register -r yml-register Plugins/News/gh-tile/test/index.spec.ts",
    "test:gh-tile:cron": "node -r esbuild-register Plugins/News/gh-tile/test/cron.spec.ts",
    "test:resvg": "node -r esbuild-register external/resvg/test/base.spec.ts",
    "test:transformers": "node -r esbuild-register external/transformers/test/base.spec.ts",
    "test:jimp": "node -r esbuild-register Plugins/Tool/jimp/test/base.spec.ts"
  },
  "devDependencies": {
    "@koishijs/client": "^5.29.4",
    "@koishijs/plugin-hmr": "^1.2.9",
    "@koishijs/scripts": "^4.6.0",
    "@satorijs/element": "^3.1.7",
    "@types/node": "^20.16.13",
    "@types/react": "^18.3.11",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.20",
    "esbuild-register": "npm:@shigma/esbuild-register@^1.1.1",
    "tsx": "^4.19.1",
    "typescript": "^5.7.0-dev.20241019",
    "yakumo": "^1.0.0-beta.18",
    "yakumo-esbuild": "^1.0.0-beta.7",
    "yakumo-esbuild-yaml": "^1.0.0-alpha.0",
    "yakumo-publish-sync": "^1.0.0-alpha.1",
    "yakumo-tsc": "^1.0.0-beta.5",
    "yml-register": "^1.2.5"
  },
  "dependencies": {
    "@initencounter/jimp": "workspace:^",
    "@initencounter/sst": "workspace:^",
    "@initencounter/vits": "workspace:^",
    "@koishijs/core": "^4.18.0",
    "@koishijs/loader": "^4.6.0",
    "@koishijs/plugin-actions": "^0.0.2",
    "@koishijs/plugin-adapter-discord": "^4.5.9",
    "@koishijs/plugin-adapter-kook": "^4.6.4",
    "@koishijs/plugin-adapter-qq": "4.8.3",
    "@koishijs/plugin-adapter-satori": "^1.3.1",
    "@koishijs/plugin-adapter-telegram": "^4.5.8",
    "@koishijs/plugin-admin": "^2.0.0-beta.4",
    "@koishijs/plugin-analytics": "^2.0.6",
    "@koishijs/plugin-auth": "^4.1.6",
    "@koishijs/plugin-bind": "^1.5.1",
    "@koishijs/plugin-commands": "^3.5.4",
    "@koishijs/plugin-config": "^2.8.5",
    "@koishijs/plugin-console": "^5.29.4",
    "@koishijs/plugin-database-sqlite": "^4.6.0",
    "@koishijs/plugin-explorer": "^1.5.5",
    "@koishijs/plugin-help": "^2.4.4",
    "@koishijs/plugin-http": "0.6.1",
    "@koishijs/plugin-insight": "^3.5.2",
    "@koishijs/plugin-inspect": "1.1.7",
    "@koishijs/plugin-locales": "^2.5.3",
    "@koishijs/plugin-logger": "^2.6.8",
    "@koishijs/plugin-market": "2.11.2",
    "@koishijs/plugin-notifier": "^1.2.1",
    "@koishijs/plugin-oobe": "^0.0.2",
    "@koishijs/plugin-proxy-agent": "0.3.3",
    "@koishijs/plugin-sandbox": "3.4.1",
    "@koishijs/plugin-server": "^3.2.3",
    "@koishijs/plugin-server-satori": "2.7.2",
    "@koishijs/plugin-server-temp": "^1.4.0",
    "@koishijs/plugin-status": "^7.4.10",
    "jimp": "1.6.0",
    "koishi": "^4.18.0",
    "koishi-plugin-adapter-onebot": "^6.7.2",
    "koishi-plugin-adapter-red": "3.2.3",
    "koishi-plugin-android": "^0.0.1",
    "koishi-plugin-assets-local": "3.3.2",
    "koishi-plugin-canvas": "0.2.2",
    "koishi-plugin-cron": "3.1.0",
    "koishi-plugin-dataview": "2.7.5",
    "koishi-plugin-desktop": "1.0.0",
    "koishi-plugin-downloads": "0.1.3",
    "koishi-plugin-ffmpeg": "^1.1.0",
    "koishi-plugin-group-manage": "1.3.1",
    "koishi-plugin-markdown-to-image-service": "1.2.5",
    "koishi-plugin-nix": "0.0.3",
    "koishi-plugin-open-vits": "^2.0.6",
    "koishi-plugin-puppeteer": "^3.9.0",
    "koishi-plugin-rate-limit": "^2.0.4",
    "koishi-plugin-silk": "^1.5.1",
    "koishi-plugin-telemetry": "0.2.3",
    "koishi-plugin-theme-vanilla": "^1.0.6",
    "koishi-plugin-toutai": "1.1.9",
    "yakumo-esbuild-yaml": "^0.3.1",
    "yakumo-publish-sync": "^0.3.3"
  }
}
